<div class='jumbotron'>
  <%= form_for :session do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email, class: 'form-control' %><br>
  
    <%= f.label :password %>
    <%= f.password_field :password, class: 'form-control' %><br>
  
    <%= f.submit "Log In", class: 'btn btn-default'%>
  <% end %>
</div>